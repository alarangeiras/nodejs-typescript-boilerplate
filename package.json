{
  "name": "nodejs-default-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node -r dotenv/config ./src/app.ts",
    "build": "tsc",
    "test": "jest --reporters=default --reporters=jest-junit",
    "dev-test": "jest --watch",
    "start": "build && node ./src/app.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^1.19.3",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1"
  }
}
